---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro';
---

<Layout title="Welcome to Astro.">
	<main>
		<section class="relative z-10 bg-[url('/profile.jpg')] bg-cover bg-[center_top_35%] min-h-[30rem]">
			<div class="w-full flex justify-center items-center">
			<div class="absolute top-0 z-10 w-full h-full lg:p-32 p-12 backdrop-brightness-50">
				<div class="container mx-auto">
					<h1 class="xl:text-[6rem] md:text-7xl text-5xl leading-none">Mathieu Bertin</h2>
					<h3 class="lg:text-5xl md:text-2xl text-2xl  font-medium text-primary-400">Full Stack Web Developer</h3>
				<div>
			</div>
		</section>
		<section class="text-center flex flex-col space-y-4 min-h-[22rem] justify-center items-center p-10 ">
			<h2 class="lg:text-5xl md:text-5xl text-3xl">Talk less.<span class="text-primary-400"> Do more.</span></h2>
			<p class="text-2xl">Passionate about concise and effective communication and getting a lot done.</p>
		</section>
		<section class="bg-alabaster-200 text-eerie-800 min-h-[20rem] p-10 py-20 text-center">
			<h2 class=" lg:text-5xl md:text-5xl text-3xl mb-10">
				Experiments
			</h2>
			<div class="grid md:grid-cols-2 text-left gap-5 max-w-6xl m-auto">
				<div class="p-5 bg-eerie-50 space-y-4">
					<div>
						<h3 class="text-3xl">
							Nephelo Forms<br />
						</h3>
						<p>
							A basic form builder with drag and drop functionality.
						</p>
